{
  "name": "@vagabond-ts/react-crud-mui",
  "version": "0.0.40",
  "description": "React lib with MUI, redux, i18n, router-dom, axios",
  "private": false,
  "scripts": {
    "lint": "eslint \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "tsc": "tsc",
    "test": "jest",
    "test:json": "npm run test -- --json --outputFile=src/jest-test-results.json",
    "coverage": "npm run test -- --runInBand --coverage ",
    "format": "npm run lint && npm run tsc",
    "ci": "npm run format && npm run test -- --coverage --testResultsProcessor jest-sonar-reporter",
    "sonar": "npm run ci && npx sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "clean": "rm -rf dist",
    "copy:asset": "cp -r src/assets dist/",
    "build": "npm run clean && tsc && npm run copy:asset && rollup -c --bundleConfigAsCjs",
    "build:dev": "npm run build -- --watch",
    "ssh:agent": "ssh-agent bash",
    "ssh:add": "ssh-add ~/.ssh/id_ed25519",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm run test:json && npm run format && npm i && npm run build"
  },
  "dependencies": {
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@fontsource/roboto": "~5.0.8",
    "@greatsumini/react-facebook-login": "~3.3.3",
    "@mui/icons-material": "~5.15.1",
    "@mui/material": "~5.15.1",
    "@mui/x-date-pickers": "^6.18.5",
    "@react-oauth/google": "~0.12.1",
    "axios": "~1.6.2",
    "dayjs": "^1.11.10",
    "dompurify": "~3.0.1",
    "formik": "~2.4.5",
    "i18next": "~23.7.11",
    "i18next-browser-languagedetector": "~7.2.0",
    "mui-markdown": "~1.1.11",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-i18next": "~13.5.0",
    "react-redux": "~9.0.4",
    "react-router-dom": "~6.21.0",
    "sass": "~1.69.5",
    "uuid": "~9.0.0",
    "yup": "~1.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.23.3",
    "@babel/preset-react": "~7.23.3",
    "@babel/preset-typescript": "~7.23.3",
    "@reduxjs/toolkit": "~2.0.1",
    "@rollup/plugin-commonjs": "~25.0.7",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~11.1.0",
    "@storybook/addon-essentials": "~7.6.6",
    "@storybook/addon-interactions": "~7.6.6",
    "@storybook/addon-jest": "~7.6.6",
    "@storybook/addon-links": "~7.6.6",
    "@storybook/blocks": "~7.6.6",
    "@storybook/builder-vite": "~7.6.6",
    "@storybook/react": "~7.6.6",
    "@storybook/react-vite": "~7.6.6",
    "@testing-library/jest-dom": "~6.1.5",
    "@testing-library/react": "~14.1.2",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.18",
    "@types/uuid": "~9.0.1",
    "@typescript-eslint/eslint-plugin": "~6.15.0",
    "@typescript-eslint/parser": "~6.15.0",
    "babel-jest": "~29.7.0",
    "css-loader": "~6.8.1",
    "eslint": "~8.56.0",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-sonar-reporter": "~2.0.0",
    "path-browserify": "~1.0.1",
    "redux-mock-store": "~1.5.4",
    "rollup": "~4.9.1",
    "rollup-plugin-dts": "~6.1.0",
    "rollup-plugin-peer-deps-external": "~2.2.4",
    "rollup-plugin-postcss": "~4.0.2",
    "rollup-plugin-scss": "~4.0.0",
    "sass": "~1.69.5",
    "sonarqube-scanner": "~3.3.0",
    "storybook": "~7.6.6",
    "typescript": "~5.3.3",
    "vite-tsconfig-paths": "~4.2.0"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "peerDependencies": {
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@fontsource/roboto": "~5.0.8",
    "@greatsumini/react-facebook-login": "~3.3.3",
    "@mui/icons-material": "~5.15.1",
    "@mui/material": "~5.15.1",
    "@react-oauth/google": "~0.12.1",
    "axios": "~1.6.2",
    "dompurify": "~3.0.1",
    "formik": "~2.4.5",
    "i18next": "~23.7.11",
    "i18next-browser-languagedetector": "~7.2.0",
    "mui-markdown": "~1.1.11",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-i18next": "~13.5.0",
    "react-redux": "~9.0.4",
    "react-router-dom": "~6.21.0",
    "sass": "~1.69.5",
    "uuid": "~9.0.0",
    "yup": "~1.3.3"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "18"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react/prop-types": "off",
      "@typescript-eslint/triple-slash-reference": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/anchor-has-content": "off",
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/no-restricted-imports": [
        "warn",
        {
          "name": "react-redux",
          "importNames": [
            "useSelector",
            "useDispatch"
          ],
          "message": "Use typed hooks `useAppDispatch` and `useAppSelector` instead."
        }
      ]
    },
    "ignorePatterns": [
      "dist/**",
      "config.js",
      "**/*babel*",
      "**/*.stories.*",
      "**/*Store.*",
      "**/*.test.*",
      "**/setupTests.*",
      "node_modules/**/*.ts?(x)"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
